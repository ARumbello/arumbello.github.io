    </div>

    <!-- Custom Date Range Modal -->
    <div class="custom-date-modal" id="customDateModal">
        <div class="custom-date-content">
            <div class="custom-date-header">
                <h3 class="custom-date-title">Select Date Range</h3>
                <button class="close-modal" id="closeModal">
                    <svg width="20" height="20" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                        <line x1="18" y1="6" x2="6" y2="18"></line>
                        <line x1="6" y1="6" x2="18" y2="18"></line>
                    </svg>
                </button>
            </div>

            <div class="date-range-inputs">
                <div class="date-input-group">
                    <label class="date-input-label">From Date</label>
                    <input type="date" class="date-input" id="fromDate">
                </div>
                <div class="date-input-group">
                    <label class="date-input-label">To Date</label>
                    <input type="date" class="date-input" id="toDate">
                </div>
            </div>

            <div class="quick-presets">
                <button class="preset-btn" data-preset="7d">Last 7 days</button>
                <button class="preset-btn" data-preset="14d">Last 14 days</button>
                <button class="preset-btn" data-preset="30d">Last 30 days</button>
                <button class="preset-btn" data-preset="90d">Last 90 days</button>
                <button class="preset-btn" data-preset="this-month">This Month</button>
                <button class="preset-btn" data-preset="last-month">Last Month</button>
            </div>

            <div class="compare-option">
                <input type="checkbox" class="compare-checkbox" id="compareCheckbox">
                <label for="compareCheckbox">Compare to previous period</label>
            </div>

            <div class="modal-actions">
                <button class="modal-btn cancel" id="cancelModal">Cancel</button>
                <button class="modal-btn apply" id="applyModal">Apply</button>
            </div>
        </div>
    </div>

    <script>
        // Date picker functionality
        document.addEventListener('DOMContentLoaded', function() {
            const datePicker = document.getElementById('datePicker');
            const dateDropdown = document.getElementById('dateDropdown');
            const datePickerText = document.getElementById('datePickerText');
            const pageSubtitle = document.querySelector('.page-subtitle');
            const customDateModal = document.getElementById('customDateModal');
            const closeModal = document.getElementById('closeModal');
            const cancelModal = document.getElementById('cancelModal');
            const applyModal = document.getElementById('applyModal');
            const fromDate = document.getElementById('fromDate');
            const toDate = document.getElementById('toDate');
            const compareCheckbox = document.getElementById('compareCheckbox');
            
            // Toggle dropdown
            datePicker.addEventListener('click', function(e) {
                e.stopPropagation();
                const isOpen = dateDropdown.classList.contains('show');
                dateDropdown.classList.toggle('show');
                datePicker.classList.toggle('open');
            });

            // Close dropdown when clicking outside
            document.addEventListener('click', function() {
                dateDropdown.classList.remove('show');
                datePicker.classList.remove('open');
            });

            // Date option selection
            const dateOptions = document.querySelectorAll('.date-option:not(.custom)');
            dateOptions.forEach(option => {
                option.addEventListener('click', function() {
                    // Remove selected class from all options
                    dateOptions.forEach(opt => opt.classList.remove('selected'));
                    // Add selected class to clicked option
                    this.classList.add('selected');
                    
                    // Update button text
                    const text = this.querySelector('span').textContent;
                    datePickerText.textContent = text;
                    
                    // Update subtitle
                    let subtitleText = '';
                    switch(this.dataset.value) {
                        case 'today':
                            subtitleText = 'Performance overview for today';
                            break;
                        case 'yesterday':
                            subtitleText = 'Performance overview for yesterday';
                            break;
                        case '7d':
                            subtitleText = 'Performance overview for the last 7 days';
                            break;
                        case '30d':
                            subtitleText = 'Performance overview for the last 30 days';
                            break;
                        case 'this-week':
                            subtitleText = 'Performance overview for this week';
                            break;
                        case 'last-week':
                            subtitleText = 'Performance overview for last week';
                            break;
                        case 'this-month':
                            subtitleText = 'Performance overview for this month';
                            break;
                        case 'last-month':
                            subtitleText = 'Performance overview for last month';
                            break;
                    }
                    pageSubtitle.textContent = subtitleText;
                    
                    // Close dropdown
                    dateDropdown.classList.remove('show');
                    datePicker.classList.remove('open');
                    
                    console.log(`Switching to ${text} view`);
                });
            });

            // Custom date range option
            const customOption = document.querySelector('.date-option.custom');
            customOption.addEventListener('click', function() {
                dateDropdown.classList.remove('show');
                datePicker.classList.remove('open');
                customDateModal.classList.add('show');
                document.body.style.overflow = 'hidden';
            });

            // Close custom date modal
            function closeCustomModal() {
                customDateModal.classList.remove('show');
                document.body.style.overflow = '';
            }

            closeModal.addEventListener('click', closeCustomModal);
            cancelModal.addEventListener('click', closeCustomModal);
            
            // Close modal when clicking outside
            customDateModal.addEventListener('click', function(e) {
                if (e.target === customDateModal) {
                    closeCustomModal();
                }
            });

            // Quick preset buttons in modal
            const presetBtns = document.querySelectorAll('.preset-btn');
            presetBtns.forEach(btn => {
                btn.addEventListener('click', function() {
                    // Remove active class from all presets
                    presetBtns.forEach(b => b.classList.remove('active'));
                    // Add active class to clicked preset
                    this.classList.add('active');
                    
                    // Set date inputs based on preset
                    const today = new Date();
                    const preset = this.dataset.preset;
                    let fromDateValue, toDateValue;
                    
                    switch(preset) {
                        case '7d':
                            fromDateValue = new Date(today.getTime() - 7 * 24 * 60 * 60 * 1000);
                            toDateValue = today;
                            break;
                        case '14d':
                            fromDateValue = new Date(today.getTime() - 14 * 24 * 60 * 60 * 1000);
                            toDateValue = today;
                            break;
                        case '30d':
                            fromDateValue = new Date(today.getTime() - 30 * 24 * 60 * 60 * 1000);
                            toDateValue = today;
                            break;
                        case '90d':
                            fromDateValue = new Date(today.getTime() - 90 * 24 * 60 * 60 * 1000);
                            toDateValue = today;
                            break;
                        case 'this-month':
                            fromDateValue = new Date(today.getFullYear(), today.getMonth(), 1);
                            toDateValue = today;
                            break;
                        case 'last-month':
                            fromDateValue = new Date(today.getFullYear(), today.getMonth() - 1, 1);
                            toDateValue = new Date(today.getFullYear(), today.getMonth(), 0);
                            break;
                    }
                    
                    fromDate.value = fromDateValue.toISOString().split('T')[0];
                    toDate.value = toDateValue.toISOString().split('T')[0];
                });
            });

            // Apply custom date range
            applyModal.addEventListener('click', function() {
                const from = fromDate.value;
                const to = toDate.value;
                const compare = compareCheckbox.checked;
                
                if (!from || !to) {
                    alert('Please select both from and to dates');
                    return;
                }
                
                if (new Date(from) > new Date(to)) {
                    alert('From date cannot be after to date');
                    return;
                }
                
                // Format date range for display
                const fromFormatted = new Date(from).toLocaleDateString('en-US', { month: 'short', day: 'numeric' });
                const toFormatted = new Date(to).toLocaleDateString('en-US', { month: 'short', day: 'numeric' });
                const rangeText = `${fromFormatted} - ${toFormatted}`;
                
                datePickerText.textContent = rangeText;
                pageSubtitle.textContent = `Performance overview for ${rangeText}${compare ? ' (with comparison)' : ''}`;
                
                // Remove selected class from all standard options
                dateOptions.forEach(opt => opt.classList.remove('selected'));
                
                closeCustomModal();
                
                console.log(`Custom range: ${from} to ${to}, Compare: ${compare}`);
            });
            
            // Initialize default date inputs
            const today = new Date();
            const sevenDaysAgo = new Date(today.getTime() - 7 * 24 * 60 * 60 * 1000);
            toDate.value = today.toISOString().split('T')[0];
            fromDate.value = sevenDaysAgo.toISOString().split('T')[0];

            // Other existing functionality...
            // Alert action buttons
            const alertActions = document.querySelectorAll('.alert-action');
            alertActions.forEach(btn => {
                btn.addEventListener('click', function(e) {
                    e.stopPropagation();
                    const action = this.textContent;
                    const alertMessage = this.closest('.alert-item').querySelector('.alert-message').textContent;
                    console.log(`${action} clicked for: ${alertMessage}`);
                });
            });
            
            // Funnel item clicks
            const funnelItems = document.querySelectorAll('.funnel-item');
            funnelItems.forEach(item => {
                item.addEventListener('click', function() {
                    const funnelName = this.querySelector('.funnel-name').textContent;
                    console.log(`Opening funnel: ${funnelName}`);
                    // Navigate to funnel details/editor
                });
            });
            
            // Create funnel button
            const createBtn = document.querySelector('.create-btn');
            createBtn.addEventListener('click', function() {
                console.log('Opening funnel builder...');
                // Navigate to funnel builder
            });
        });
    </script>
</body>
</html>
