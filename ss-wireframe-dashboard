                            <svg width="16" height="16" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2">
                                <polyline points="23,18 13.5,8.5 8.5,13.5 1,6"></polyline>
                                <polyline points="17,18 23,18 23,12"></polyline>
                            </svg>
                        </div>
                    </div>

                    <div class="funnel-item">
                        <div class="funnel-icon" style="background: linear-gradient(135deg, #06b6d4, #0891b2);">N</div>
                        <div class="funnel-info">
                            <div class="funnel-name">Neck Pain Relief Device</div>
                            <div class="funnel-meta">
                                <span>Updated 1 week ago</span>
                                <span class="funnel-status status-active">● Active</span>
                            </div>
                        </div>
                        <div class="funnel-metrics">
                            <div class="metric">
                                <div class="metric-value">64.9%</div>
                                <div class="metric-label">Trial Conv</div>
                            </div>
                            <div class="metric">
                                <div class="metric-value">$142</div>
                                <div class="metric-label">AOV</div>
                            </div>
                            <div class="metric">
                                <div class="metric-value">$10,980</div>
